@model List<Asp21.Models.Cliente>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>

    @foreach(var cliente in Model)
    {
        <p>@cliente.Nome @cliente.Apelido - @Html.ActionLink("Detalhes","detalhes", new { id = cliente.id })</p>
    }

</body>
</html>
